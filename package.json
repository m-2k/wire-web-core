{
  "author": "Wire",
  "dependencies": {
    "@wireapp/api-client": "0.1.13",
    "@wireapp/store-engine": "0.1.0",
    "bazinga64": "5.0.3",
    "protobufjs": "6.8.0",
    "pure-uuid": "1.4.7",
    "wire-webapp-cryptobox": "6.2.0",
    "wire-webapp-protocol-messaging": "1.19.0"
  },
  "devDependencies": {
    "cross-env": "5.0.5",
    "istanbul": "0.4.5",
    "jasmine": "2.8.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-diff-reporter": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "prettier": "1.6.1",
    "rimraf": "2.6.1",
    "typescript": "2.5.2",
    "webpack": "3.5.6"
  },
  "description": "Wire API Client to send and receive data.",
  "license": "GPL-3.0",
  "lint-staged": {
    "*.ts": [
      "npm run prettier",
      "git add"
    ]
  },
  "main": "./dist/commonjs/index.js",
  "name": "@wireapp/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wireapp/wire-web-core.git"
  },
  "scripts": {
    "clear": "rimraf dist",
    "coverage": "istanbul cover --report html ./node_modules/jasmine/bin/jasmine.js",
    "dist": "npm run clear && tsc",
    "dist:browser": "npm run dist && webpack --config ./webpack.browser.js",
    "postversion": "git push && git push --tags && npm run dist:browser",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --trailing-comma=\"all\" --no-bracket-spacing --write \"src/**/*.ts\"",
    "preversion": "npm run && npm run dist && npm run test",
    "start": "rimraf .tmp && npm run dist && node index.js",
    "version": "npm run dist",
    "test": "npm run clear && npm run dist && npm run test:node && npm run test:browser",
    "test:node": "cross-env JASMINE_CONFIG_PATH=src/test/node/support/jasmine.json jasmine",
    "test:browser": "echo No operation."
  },
  "types": "./dist/commonjs/index.d.ts",
  "version": "0.0.0"
}
